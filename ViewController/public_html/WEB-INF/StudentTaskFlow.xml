<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="StudentTaskFlow">
    <default-activity id="__1">StudentHome</default-activity>
    <view id="StudentHome">
      <page>/WEB-INF/Pages/StudentHomePage.jsff</page>
    </view>
    <view id="StudentProfile">
      <page>/Pages/StudentProfilePage.jsff</page>
    </view>
    <view id="Assignments">
      <page>/Pages/AssignmentsPage.jsff</page>
    </view>
    <view id="Courses">
      <page>/Pages/CoursesPage.jsff</page>
    </view>
    <task-flow-return id="Logout">
      <outcome id="__16">
        <name>logout</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__2">
      <from-activity-id id="__3">*</from-activity-id>
      <control-flow-case id="__4">
        <from-outcome id="__6">profile</from-outcome>
        <to-activity-id id="__5">StudentProfile</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__7">
        <from-outcome id="__9">assignments</from-outcome>
        <to-activity-id id="__8">Assignments</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__10">
        <from-outcome id="__12">home</from-outcome>
        <to-activity-id id="__11">StudentHome</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__14">
        <from-outcome id="__15">courses</from-outcome>
        <to-activity-id id="__13">Courses</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__17">
        <from-outcome id="__19">logout</from-outcome>
        <to-activity-id id="__18">Logout</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
